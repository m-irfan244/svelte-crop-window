{
    "name": "svelte-crop-window",
    "version": "0.1.1",
    "description": "A Svelte component to crop images and videos with touch and mouse gestures",
    "homepage": "https://sabine.github.io/svelte-crop-window",
    "repository": {
        "type": "git",
        "url": "https://github.com/sabine/svelte-crop-window"
    },
    "author": "Sabine Schmaltz <info@sabineschmaltz.de>",
    "license": "MIT",
    "keywords": [
        "cropper",
        "crop window",
        "image crop",
        "video crop",
        "svelte",
        "svelte component"
    ],
    "scripts": {
        "dev": "vite dev --port 3000 --host",
        "gh-pages": "vite build && node gh-pages.cjs",
        "build": "svelte-kit sync && svelte-package && node clean-dependencies.js",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --plugin-search-dir . --check . && eslint .",
        "format": "prettier --plugin-search-dir . --write ."
    },
    "devDependencies": {
        "@sveltejs/kit": "1.0.0-next.511",
        "@sveltejs/package": "1.0.0-next.1",
        "@sveltejs/adapter-static": "1.0.0-next.44",
        "@typescript-eslint/eslint-plugin": "^5.27.0",
        "@typescript-eslint/parser": "^5.27.0",
        "eslint": "^8.16.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "prettier": "^2.6.2",
        "prettier-plugin-svelte": "^2.7.0",
        "svelte": "^3.44.0",
        "svelte-check": "^2.7.1",
        "svelte-highlight": "^6.2.1",
        "svelte-preprocess": "^4.10.6",
        "tslib": "^2.3.1",
        "typescript": "^4.7.4",
        "vite": "^3.1.0",
        "gh-pages": "^4.0.0"
    },
    "type": "module",
    "dependencies": {}
}
